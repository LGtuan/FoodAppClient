{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/components/index"
      ],
      "@services": [
        "src/services/index"
      ],
      "@redux": [
        "src/redux/index"
      ],
      "@screens": [
        "src/screens/index"
      ],
      "@navigations": [
        "src/navigations/index"
      ],
      "@constants": [
        "src/constants/index"
      ],
      "@asset": [
        "src/asset/*"
      ],
      "@utils": [
        "src/utils/index"
      ],
    }
  }
}